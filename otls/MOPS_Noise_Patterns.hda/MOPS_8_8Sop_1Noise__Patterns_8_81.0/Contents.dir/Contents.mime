snippet	[ 0	locks=0	autoscope=0 ]	(	"vector4 pos = @P;
vector4 freq = chp(\"freq\");
vector4 offset = chp(\"offset\");
float amp = chf(\"amp\");

int oct = chi(\"octaves\");
float rough = chf(\"roughness\");

vector val = 0;
vector maxval = 0;

offset += p@time_offset;

if(chi(\"uniform\") == 1){
    freq = set(freq.x, freq.x, freq.x);
}

for(int i = 0; i < oct; i++){
    freq *= 2;
    amp *= rough;
    maxval += amp;
    vector4 npos = pos * freq + offset;
    vector nval = snoise(npos);
    nval = fit(nval, -1, 1, -1, 1);
    nval *= amp;
    val += nval;
}

val /= maxval;

v@attr = val * amp;"	)
